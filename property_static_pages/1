<script type='text/javascript'>

(function () {

	var file_list = {};	
	
	var load_script = function (url) {

	    if (url) {
	        var doc = document,
	        t = doc.createElement("script"),
	        s = doc.getElementsByTagName("script")[0];
	        t.type = "text/javascript";
	
	        // Keep script order!
	        t.async = false;
	        t.src = url;
	        s.parentNode.insertBefore(t, s);
	    }
	
	};

	
	file_list[0]='http://localhost:8888/Prospero/current/resources/javascript/modules/site_wide/site_wide.js';
	file_list[1]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/bumpbox.js';
	file_list[2]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/background_gallery.js';
	file_list[3]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/thumbnail_controller.js';
	file_list[4]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/contact.js';
	file_list[5]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/form_animation.js';
	file_list[6]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/general_maps.js';
	file_list[7]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/general_map.js';
	file_list[8]='http://localhost:8888/Prospero/current/resources/javascript/modules/pages/site_wide.js';
	file_list[9]='http://localhost:8888/Prospero/current/resources/javascript/modules/pages/bumpbox.js';
	file_list[10]='http://localhost:8888/Prospero/current/resources/javascript/modules/pages/property.js';	
	for (var index in file_list) {
		
		var url = file_list[index];
		load_script(url);

	}
}());

</script>	</body>
</html>
