<script type='text/javascript'>

(function () {

	var file_list = {};	
	
	var load_script = function (url) {

	    if (url) {
	        var doc = document,
	        t = doc.createElement("script"),
	        s = doc.getElementsByTagName("script")[0];
	        t.type = "text/javascript";
	
	        // Keep script order!
	        t.async = false;
	        t.src = url;
	        s.parentNode.insertBefore(t, s);
	    }
	
	};

	
	file_list[0]='http://localhost:8888/Prospero/current/resources/javascript/modules/site_wide/site_wide.js';
	file_list[1]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/map.js';
	file_list[2]='http://localhost:8888/Prospero/current/resources/javascript/modules/modules/places_controller.js';
